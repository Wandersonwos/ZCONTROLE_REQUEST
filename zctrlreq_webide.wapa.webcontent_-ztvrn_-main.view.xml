<mvc:View xmlns:html="http://www.w3.org/1999/xhtml"                                                                                                                                                                                                            
	xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"                                                                                                                                                                                             
	controllerName="ztvrn.main">                                                                                                                                                                                                                                  
	<App>                                                                                                                                                                                                                                                         
		<pages>                                                                                                                                                                                                                                                      
			<Page title="ZTVRN - Versão SAPUI5">                                                                                                                                                                                                                        
				<content>                                                                                                                                                                                                                                                  
					<VBox direction="Column" id="vbox0" justifyContent="Center"                                                                                                                                                                                               
						width="90%">                                                                                                                                                                                                                                             
						<items>                                                                                                                                                                                                                                                  
							<Label id="label3"                                                                                                                                                                                                                                      
								text="Try out the Layout Editor and then click Run."                                                                                                                                                                                                   
								textAlign="Center" width="100%" />                                                                                                                                                                                                                     
						</items>                                                                                                                                                                                                                                                 
					</VBox>                                                                                                                                                                                                                                                   
					<l:VerticalLayout class="sapUiContentPadding"                                                                                                                                                                                                             
						width="30%">                                                                                                                                                                                                                                             
						<l:content>                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
							<Label text="Demanda" labelFor="ifDemanda" />                                                                                                                                                                                                           
							<Input id="ifDemanda" type="Text"                                                                                                                                                                                                                       
								class="sapUiSmallMarginBottom"                                                                                                                                                                                                                         
								placeholder="Insira o código da demanda (CR, projeto ou chamado)" />                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
							<Label text="Request" labelFor="ifRequest" />                                                                                                                                                                                                           
							<Input id="ifRequest" type='Text'                                                                                                                                                                                                                       
								class="sapUiSmallMarginBottom"                                                                                                                                                                                                                         
								placeholder="Insira o código da request" />                                                                                                                                                                                                            
							<Button text="Buscar" press="onPress"                                                                                                                                                                                                                   
								ariaDescribedBy="defaultButtonDescription genericButtonDescription">                                                                                                                                                                                   
								<layoutData>                                                                                                                                                                                                                                           
									<FlexItemData growFactor="1" />                                                                                                                                                                                                                       
								</layoutData>                                                                                                                                                                                                                                          
							</Button>                                                                                                                                                                                                                                               
						</l:content>                                                                                                                                                                                                                                             
					</l:VerticalLayout>                                                                                                                                                                                                                                       
					<Table noDataText="Nenhuma request encontrada" id="tRequests"                                                                                                                                                                                             
						items="{/RequestSet}">                                                                                                                                                                                                                                   
						<items>                                                                                                                                                                                                                                                  
							<ColumnListItem id="__item0">                                                                                                                                                                                                                           
								<cells>                                                                                                                                                                                                                                                
									<ObjectIdentifier                                                                                                                                                                                                                                     
										xmlns:html="http://www.w3.org/1999/xhtml"                                                                                                                                                                                                            
										xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" title="{Trkorr}"                                                                                                                                                                                           
										text="{As4text}" id="__identifier0" />                                                                                                                                                                                                               
									<Text text="{Consultor}" id="__text1" />                                                                                                                                                                                                              
									<Text text="{Funcional}" id="__text2" />                                                                                                                                                                                                              
									<Text xmlns:html="http://www.w3.org/1999/xhtml"                                                                                                                                                                                                       
										xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" text="{Modulo}"                                                                                                                                                                                            
										id="__text2_copy" />                                                                                                                                                                                                                                 
									<ObjectListItem                                                                                                                                                                                                                                       
										xmlns:html="http://www.w3.org/1999/xhtml"                                                                                                                                                                                                            
										xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" id="__item1"                                                                                                                                                                                               
										type="Active">                                                                                                                                                                                                                                       
										<attributes>                                                                                                                                                                                                                                         
											<ObjectAttribute text="{Dev}"                                                                                                                                                                                                                       
												id="__attribute0" />                                                                                                                                                                                                                               
											<ObjectAttribute                                                                                                                                                                                                                                    
												xmlns:html="http://www.w3.org/1999/xhtml"                                                                                                                                                                                                          
												xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" text="{Qas}"                                                                                                                                                                                             
												id="__attribute0_copy" />                                                                                                                                                                                                                          
											<ObjectAttribute                                                                                                                                                                                                                                    
												xmlns:html="http://www.w3.org/1999/xhtml"                                                                                                                                                                                                          
												xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" text="{Prd}"                                                                                                                                                                                             
												id="__attribute0_copy2" />                                                                                                                                                                                                                         
										</attributes>                                                                                                                                                                                                                                        
									</ObjectListItem>                                                                                                                                                                                                                                     
									<Text xmlns:html="http://www.w3.org/1999/xhtml"                                                                                                                                                                                                       
										xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" text="{Demanda}"                                                                                                                                                                                           
										id="__text2_copy3" />                                                                                                                                                                                                                                
								</cells>                                                                                                                                                                                                                                               
							</ColumnListItem>                                                                                                                                                                                                                                       
						</items>                                                                                                                                                                                                                                                 
						<columns>                                                                                                                                                                                                                                                
							<Column id="__column0">                                                                                                                                                                                                                                 
								<header>                                                                                                                                                                                                                                               
									<Label text="Request" id="__label0" />                                                                                                                                                                                                                
								</header>                                                                                                                                                                                                                                              
							</Column>                                                                                                                                                                                                                                               
							<Column id="__column1">                                                                                                                                                                                                                                 
								<header>                                                                                                                                                                                                                                               
									<Label text="Consultor" id="__label1" />                                                                                                                                                                                                              
								</header>                                                                                                                                                                                                                                              
							</Column>                                                                                                                                                                                                                                               
							<Column id="__column2">                                                                                                                                                                                                                                 
								<header>                                                                                                                                                                                                                                               
									<Label text="Funcional" id="__label2" />                                                                                                                                                                                                              
								</header>                                                                                                                                                                                                                                              
							</Column>                                                                                                                                                                                                                                               
							<Column xmlns:html="http://www.w3.org/1999/xhtml"                                                                                                                                                                                                       
								xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" id="__column2_copy">                                                                                                                                                                                         
								<header>                                                                                                                                                                                                                                               
									<Label text="Módulo" id="__label2_copy" />                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
								</header>                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
							</Column>                                                                                                                                                                                                                                               
							<Column xmlns:html="http://www.w3.org/1999/xhtml"                                                                                                                                                                                                       
								xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" id="__column2_copy2">                                                                                                                                                                                        
								<header>                                                                                                                                                                                                                                               
									<Label text="Status" id="__label2_copy2" />                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
								</header>                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
							</Column>                                                                                                                                                                                                                                               
							<Column xmlns:html="http://www.w3.org/1999/xhtml"                                                                                                                                                                                                       
								xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" id="__column2_copy3">                                                                                                                                                                                        
								<header>                                                                                                                                                                                                                                               
									<Label text="Demanda" id="__label2_copy3" />                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
								</header>                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
							</Column>                                                                                                                                                                                                                                               
						</columns>                                                                                                                                                                                                                                               
					</Table>                                                                                                                                                                                                                                                  
				</content>                                                                                                                                                                                                                                                 
			</Page>                                                                                                                                                                                                                                                     
		</pages>                                                                                                                                                                                                                                                     
	</App>                                                                                                                                                                                                                                                        
</mvc:View>                                                                                                                                                                                                                                                    